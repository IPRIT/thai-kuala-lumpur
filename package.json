{
  "name": "thai-kuala-lumpur",
  "version": "0.0.1",
  "description": "Kuala Lumpur Bot",
  "main": "build/server.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development DEBUG=thai-kuala-lumpur* nodemon",
    "start": "cross-env NODE_ENV=production DEBUG=thai-kuala-lumpur:log* run-s start:node",
    "start:node": "node --max-old-space-size=8192 ./build/src/server.js",
    "build": "run-s build:compile build:paths",
    "build:compile": "tsc -p tsconfig.json",
    "build:paths": "babel build -d build",
    "preview": "run-s build start",
    "pm2:start": "pm2 start pm2.config.js --env production --only bot",
    "pm2:start:dev": "pm2 delete pm2.config.js && pm2 start pm2.config.js --only bot",
    "encrypt": "node tools/encrypt.js",
    "decrypt": "node tools/decrypt.js"
  },
  "author": "Alexander Belov",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "node-fetch": "^2.6.7",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "telegraf": "^4.11.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.9",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/lodash-es": "^4.17.6",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.23",
    "@types/node-fetch": "^2.6.2",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "cross-env": "^7.0.3",
    "debug": "^4.3.4",
    "eslint": "^8.12.0",
    "eslint-config-standard": "^17.0.0-1",
    "eslint-plugin-compat": "^4.0.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.8.4"
  }
}
